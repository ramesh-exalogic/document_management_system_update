



<%= form_with(model: @article, local: true, html: { multipart: true }) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :body %>
    <%= f.text_field :body %>
  </div>


  <div>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>


  <%= f.hidden_field :user_id, value: current_user.id %>
  
  <div>
    <%= f.submit %>
  </div>
<% end %>
